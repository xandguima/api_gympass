    "@vitest/ui": "3.0.8",
    "npm-run-all": "4.1.5",
    "prisma": "6.5.0",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8"
  },
  "dependencies": {
